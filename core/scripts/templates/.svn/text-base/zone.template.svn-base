$ORIGIN .
$TTL 1D
<domain>	IN SOA	ns.<domain>. root.<domain>. (
					1	; serial
					1M	; refresh
					1H	; retry
					1W	; expire
					3H )	; minimum
	NS	ns.<domain>.
$ORIGIN <domain>.
ns		A	<address>
server	A	<address>


$ORIGIN _dns-sd._udp.<domain>.
b		IN PTR srv.<domain>.
db		IN PTR srv.<domain>.
lb		IN PTR srv.<domain>.
r		IN PTR srv.<domain>.
dr		IN PTR srv.<domain>.
